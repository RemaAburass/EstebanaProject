<%if current_user and current_user.role !=0 %>
  <body class="bg-ques">
    <section class="section container">
      <table class="table table-striped table-hover title-color mt-5 bg-white">
        <thead style="background-color:#223C60;" class="text-white text-center">
          <tr>
            <th>Survey Date</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody class="title-color">
          <% @surveys.each do |survey| %>
            <tr>
              <td> This survey was filled in: <%= survey.created_at %></td>
              <td>
                <div class="btn-group " role="group" aria-label="Basic example">
                <%= button_to 'Show', survey, method: :get, :class=>"btn-q btn-secondary" %>
                </div>
              </td>
            </tr>
          <% end %>
        </tbody>
      </table>
      <br/>
      <hr/>
      <br/>
    </section>
  </body>
<%else%>
<% end %>